package edu.uoc.meteoPocket{	import flash.events.Event;		public class ScreenEvent extends Event 	{		public static const CHANGE_SCREEN:String = 'changeScreen';				public var screenID:int;				public function ScreenEvent(type:String, bubbles:Boolean=false, cancelable:Boolean=false, screenID:int=-1) 		{			super(type, bubbles, cancelable);			this.screenID = screenID;		}				override public function clone():Event		{			return new ScreenEvent(type, bubbles, cancelable, screenID);		}				override public function toString():String		{			return formatToString("ScreenEvent", "type", "bubbles", "cancelable", "eventPhase", "screenID"); 		}	}	}