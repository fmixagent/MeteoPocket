package edu.uoc.meteoPocket{	import flash.events.Event;		public class PrefEvent extends Event 	{		public static const CLOSE_PREF:String = 'closePreferences';				public var woeid:String;				public function PrefEvent(type:String, bubbles:Boolean=false, cancelable:Boolean=false, woeid:String="") 		{			super(type, bubbles, cancelable);			this.woeid = woeid;		}				override public function clone():Event		{			return new PrefEvent(type, bubbles, cancelable, woeid);		}				override public function toString():String		{			return formatToString("ScreenEvent", "type", "bubbles", "cancelable", "eventPhase", "woeid"); 		}	}	}